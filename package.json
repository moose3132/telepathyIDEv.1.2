{
  "name": "telepathy",
  "version": "1.2.1",
  "description": "A Realtime Live Editor for Web Learners",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/win/logo.ico --prune=true --out=dist",
    "build": "electron-builder . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=logo.ico --prune=true --out=release-builds --version-string.CompanyName=Company --version-string.FileDescription=CE --version-string.ProductName=\"Product\"",
    
    "dist": "telepathy"
  },
  "repository": "",
  "keywords": [
    "web editor"
  ],
  "author": "Michael Larry Ancona",
  "license": "Freeware",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-packager": "^14.0.5",
    "electron-winstaller": "^4.0.0"
  },
  "dependencies": {}
}
